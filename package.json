{
  "name": "BrowserifyTest",
  "version": "1.0.0",
  "description": "Testing to see if browserify-shim can be used to load bootstrap,semantic-ui or backbone",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "vamsiampolu",
  "license": "ISC",
  "devDependencies": {
    "browserify": "^8.0.3",
    "browserify-shim": "^3.8.2",
    "gulp": "^3.8.10",
    "gulp-streamify": "0.0.5",
    "gulp-uglify": "^1.0.2",
    "hbsfy": "^2.2.1",
    "vinyl-source-stream": "^1.0.0"
  },
  "dependencies": {
    "backbone": "^1.1.2",
    "bootstrap": "^3.2.0",
    "jquery": "^2.1.3"
  },
  "browserify": {
    "transform": [
      "browserify-shim",
      "hbsfy"
    ]
  },
  "browser": {
    "bootstrap": "./node_modules/bootstrap/dist/js/bootstrap.js",
    "backbone": "./node_modules/backbone/backbone.js",
    "bootstrap-validator": "./bootstrap-validator-master/dist/validator.js",
    "bootcards": "./bootcards-master/dist/js/bootcards.js",
    "Backbone.actAs.Paginatable": "./Backbone.actAs.Paginatable-master/src/Backbone.actAs.Paginatable.js",
    "backbone-query": "./backbone_query-master/backbone-query.js"
  },
  "browserify-shim": {
    "bootstrap": {
      "depends": [
        "jquery:jQuery"
      ]
    },
    "backbone": {
      "depends": [
        "jquery:$"
      ],
      "exports": "Backbone"
    },
    "bootstrap-validator": {
      "depends": [
        "jquery:jQuery",
        "bootstrap"
      ]
    },
    "bootcards": {
      "depends": [
        "jquery:$",
        "bootstrap"
      ]
    },
    "Backbone.actAs.Paginatable": {
      "depends": [
        "backbone:Backbone"
      ]
    },
    "backbone-query": {
      "depends": [
        "backbone:Backbone"
      ]
    }
  }
}
